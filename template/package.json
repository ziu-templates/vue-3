{
  "name": "vue-3-tpl-1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    {{#lint}}
    "lint": "vue-cli-service lint",
    {{/lint}}
    {{#gitCommitMsg}}
    "husky": "node node_modules/husky/lib/installer/bin install",
    "unhusky": "node node_modules/husky/lib/installer/bin uninstall",
    "commit": "ziu commit",
    {{/gitCommitMsg}}
    {{#img}}
    "img": "npm run ziu-img -- src",
    {{/img}}
    {{#changelog}}
    "postversion": "ziu cl -p angular -i CHANGELOG.md -s -r 1000 && git add CHANGELOG.md package.json && git add -u"
    {{/changelog}}
    "serve": "vue-cli-service serve",
    "build": "node build/ziuServer.js",
    "stg": "npm run build -- staging",
    "testing": "npm run build -- testing"
  },
  "dependencies": {
    {{#antd}}
    "ant-design-vue": "^1.3.10",
    {{/antd}}
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    {{#lint}}
    "@vue/cli-plugin-eslint": "^3.8.0",
    {{#if_eq lintConfig "airbnb"}}
    "@vue/eslint-config-airbnb": "^4.0.0",
    {{/if_eq}}
    "babel-eslint": "^10.0.1",
    {{#antd}}
    "babel-plugin-import": "^1.12.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    {{/antd}}
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    {{/lint}}
    {{#gitCommitMsg}}
    "commitlint": "^8.0.0",
    "husky": "^2.4.1",
    {{/gitCommitMsg}}
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    {{#performanceConfig}}
    "ziu": "^1.5.0",
    {{/performanceConfig}}
    "vue-template-compiler": "^2.6.10"
  }
}
